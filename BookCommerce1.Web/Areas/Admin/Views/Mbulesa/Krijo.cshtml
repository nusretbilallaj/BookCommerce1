@using BookCommerce1.Model
@model Mbulesa
<form method="post">
    <div class="border p-3">
        <div class="row">
            <h2 class="text-primary">Shtimi i mbuleses</h2>
        </div>
        <div class="mb-2" >
            <label asp-for="Emri"></label>
            <input asp-for="Emri" class="form-control" />
            <span asp-validation-for="Emri" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary">Shto</button>
        <a asp-controller="Mbulesa" asp-action="Listo" class="btn btn-secondary">Kthehu</a>
    </div>
</form>

@section Scripts
{
    @{
        <partial name="_ValidationScriptsPartial"/>
    }
   
}
